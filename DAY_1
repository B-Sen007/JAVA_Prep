
import java.util.Scanner;
public class HelloWorld{

     public static void main(String []args){
         Scanner sc= new Scanner(System.in);
         System.out.println("enter the 2 numbers : ");
         int a=sc.nextInt();
         int b=sc.nextInt();
         int sum= a+b;
         System.out.println("sum is " + sum);
     }
}
